<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maximus.vitaminreminder">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_medicine"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_medicine"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pref.SettingsActivity" />
        <activity android:name=".timepicker.TimePickerActivity"
            android:parentActivityName=".MainActivity"></activity>
        <receiver android:name=".timepicker.AlarmReceiver"
            android:enabled="false">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>