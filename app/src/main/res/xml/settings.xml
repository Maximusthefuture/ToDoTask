<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <com.example.maximus.vitaminreminder.pref.TimePreference
        android:key="key4"
        android:title="TimePreference"
        android:summary="Time Summary"
        android:defaultValue="90"
        />

    <CheckBoxPreference
        android:key="notification_key"
        android:title="Notification"
        android:defaultValue="false"
        android:summaryOn="YES"
        android:summaryOff="NO"/>


    <ListPreference
        android:defaultValue="@string/ten_min"
        android:dialogTitle="Choose interval"
        android:key="@string/interval_list_key"
        android:title="Choose Interval"
        android:entries="@array/pref_interval_time_lables"
        android:entryValues="@array/pref_interval_option_value"
        />

</PreferenceScreen>